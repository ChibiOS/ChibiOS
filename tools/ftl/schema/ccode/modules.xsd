      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="conditionalFunctionsType">
    <xs:choice maxOccurs="unbounded" minOccurs="1">
      <xs:element name="function" type="functionType" maxOccurs="1"
